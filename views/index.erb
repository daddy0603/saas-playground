<div id="casement">
  <div id="source">
    <div class="sass panel" data-name="sass">
      <%= partial :"shared/_sass_input" %>
    </div>
    <div class="css panel" data-name="css" id="css_input">
      <%= partial :"shared/_css_output" %>
    </div>
    <div class="html panel" data-name="html" id="html_input">
      <%= partial :"shared/_html_input" %>
    </div>
  </div>

  <div class="rendered" id="rendered" data-name="rendered">
    <iframe class="html_output" id="rendered-html" sandbox="allow-scripts allow-same-origin"></iframe>

    <div id="sash_cover"></div>
  </div>
</div>
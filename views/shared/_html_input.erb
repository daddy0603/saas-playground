<form action="/compile-html" method="post" accept-charset="utf-8" class="html_input" id="html-form">
  <div class="edit_container">
    <div class="edit-header">

      <div id="" class="panel-toggle">
        <span id="html-syntax" class="selected"></span>
      
        <ul>
          <li><span data-toggle-value="HTML" data-toggle-input="html_syntax">HTML</span></li>
          <li><span data-toggle-value="Haml" data-toggle-input="html_syntax">Haml</span></li>
          <li><span data-toggle-value="Slim" data-toggle-input="html_syntax">Slim</span></li>
          <li><span data-toggle-value="Markdown" data-toggle-input="html_syntax">Markdown</span></li>
          <li><span data-toggle-value="Textile" data-toggle-input="html_syntax">Textile</span></li>
        </ul>
      </div>

    </div>

    <div class="pre_container">
      <pre id="html"></pre>
    </div>
  </div>
</form>

